<Window x:Class="StichtitePizzaForm.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Register" Height="300" Width="300">
    <Canvas Background="Aquamarine">
        <Button Content="Register" Canvas.Left="100" Canvas.Top="241" Width="100" Click="OnRegsterClick" />
        <TextBlock Canvas.Left="100" TextWrapping="Wrap" Text="Username" Canvas.Top="30" Height="20" Width="100"/>
        <TextBox Name="username" Height="23" Canvas.Left="100" TextWrapping="Wrap" Text="" Canvas.Top="55" Width="100"/>
        <TextBlock Canvas.Left="100" TextWrapping="Wrap" Text="Password" Canvas.Top="83" Height="20" Width="100"/>
        <TextBox Name="password" Height="23" Canvas.Left="100" TextWrapping="Wrap" Text="" Canvas.Top="107" Width="100"/>
        <TextBlock Canvas.Left="100" TextWrapping="Wrap" Text="Address" Canvas.Top="135" Height="20" Width="100"/>
        <TextBox Name="address" Height="23" Canvas.Left="100" TextWrapping="Wrap" Text="" Canvas.Top="160" Width="100"/>
        <TextBlock Canvas.Left="100" TextWrapping="Wrap" Text="Phone Number" Canvas.Top="188" Height="20" Width="100"/>
        <TextBox Name="phone" Height="23" Canvas.Left="100" TextWrapping="Wrap" Text="" Canvas.Top="213" Width="100"/>

    </Canvas>
</Window>
